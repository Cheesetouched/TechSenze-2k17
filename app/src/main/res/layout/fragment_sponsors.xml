<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D3D3D3"
    tools:context="com.rishabh.techsenze.Sponsors">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/kapaver"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:contentDescription="@string/image"
                android:src="@drawable/kapaver" />

            <ImageView
                android:id="@+id/karthika"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/kapaver"
                android:contentDescription="@string/image"
                android:src="@drawable/karthika" />

            <ImageView
                android:id="@+id/indianbank"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/karthika"
                android:contentDescription="@string/image"
                android:src="@drawable/indianbank" />

            <ImageView
                android:id="@+id/aachi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/indianbank"
                android:contentDescription="@string/image"
                android:src="@drawable/aachi" />

            <ImageView
                android:id="@+id/actor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/aachi"
                android:contentDescription="@string/image"
                android:src="@drawable/aachi" />

            <ImageView
                android:id="@+id/hp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/actor"
                android:contentDescription="@string/image"
                android:src="@drawable/hp" />'

            <ImageView
                android:id="@+id/caddlabs"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/hp"
                android:contentDescription="@string/image"
                android:src="@drawable/caddlabs" />

            <ImageView
                android:id="@+id/weevent"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/caddlabs"
                android:contentDescription="@string/image"
                android:src="@drawable/weevent" />

            <ImageView
                android:id="@+id/sri"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/weevent"
                android:contentDescription="@string/image"
                android:src="@drawable/sri" />

            <ImageView
                android:id="@+id/bpcl"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/sri"
                android:contentDescription="@string/image"
                android:src="@drawable/bpcl" />

            <ImageView
                android:id="@+id/iodt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/bpcl"
                android:contentDescription="@string/image"
                android:src="@drawable/iodt" />

            <ImageView
                android:id="@+id/srp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/iodt"
                android:contentDescription="@string/image"
                android:src="@drawable/srp" />

            <ImageView
                android:id="@+id/softence"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/srp"
                android:contentDescription="@string/image"
                android:src="@drawable/softence" />

            <ImageView
                android:id="@+id/abirami"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/softence"
                android:contentDescription="@string/image"
                android:src="@drawable/abirami_textiles" />

            <ImageView
                android:id="@+id/hdfc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/abirami"
                android:contentDescription="@string/image"
                android:src="@drawable/hdfcbank" />

            <ImageView
                android:id="@+id/apollo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/hdfc"
                android:contentDescription="@string/image"
                android:src="@drawable/apollo" />

            <ImageView
                android:id="@+id/poorvika"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/apollo"
                android:contentDescription="@string/image"
                android:src="@drawable/poorvika" />

            <ImageView
                android:id="@+id/kvb"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/poorvika"
                android:contentDescription="@string/image"
                android:src="@drawable/kvb" />

            <ImageView
                android:id="@+id/gf"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/kvb"
                android:contentDescription="@string/image"
                android:src="@drawable/gateforum" />

            <ImageView
                android:id="@+id/diet"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/gf"
                android:contentDescription="@string/image"
                android:src="@drawable/diet" />

            <ImageView
                android:id="@+id/sandwich"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/diet"
                android:contentDescription="@string/image"
                android:src="@drawable/sandwich" />

            <ImageView
                android:id="@+id/eyeopen"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/sandwich"
                android:contentDescription="@string/image"
                android:src="@drawable/eyeopen" />

            <ImageView
                android:id="@+id/cavinkare"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/eyeopen"
                android:contentDescription="@string/image"
                android:src="@drawable/cavinkare" />

            <ImageView
                android:id="@+id/mcd"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/cavinkare"
                android:contentDescription="@string/image"
                android:src="@drawable/mcd" />

            <ImageView
                android:id="@+id/dominos"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/mcd"
                android:contentDescription="@string/image"
                android:src="@drawable/dominos" />

            <ImageView
                android:id="@+id/naturals"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/dominos"
                android:contentDescription="@string/image"
                android:src="@drawable/naturals" />

            <ImageView
                android:id="@+id/malabar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/naturals"
                android:contentDescription="@string/image"
                android:src="@drawable/malabar" />

        </LinearLayout>

    </ScrollView>

</RelativeLayout>
